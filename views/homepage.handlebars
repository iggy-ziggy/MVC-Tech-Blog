<!-- Loop over each object in the projects array -->
{{#each blogs as |blog| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/blogs/{{blog.id}}">{{blog.title}}</a>
    </h2>
  </div>
  <div class="col-md-7">
    <p>
      Created by {{blog.user.name}} on {{format_date blog.date_created}}
    </p>
  </div>
</div>
{{/each}}
